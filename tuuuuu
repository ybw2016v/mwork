function Rossler
clear;clc
a=[10,0];
b=[100,1];
c=[5.7,10];
x0=[0 -1 00.01 0];
for jj=1:2
    [t,x]=ode45(@myRossler,[0,1],x0,[],a(jj),b(jj),c(jj));
    figure;polar(x(:,4),x(:,3));grid on;
end
function dx=myRossler(t,x,a,b,c)
dx=[
    2*a*x(2)/x(3)-b*x(1);
    a*a*x(3)+2*a*x(1);
    x(2);
    x(1)
    ];
