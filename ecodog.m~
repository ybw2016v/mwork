function lum = myfunction()

imgTemp = imread('IMG_20170729_094634.jpg');

[m,n]=size(imgTemp);
m = double(m);
n = double(n);
divide = double(m*n);

Xrgb = 0.2990*imgTemp(:,:,1) + 0.5870*imgTemp(:,:,2) + 0.1140*imgTemp(:,:,3);%提取亮度信息
Xrgb = double(Xrgb);
i = double(0);
lum = double(0);
while (i < divide)
    lum = lum + Xrgb(i);
    i = i + 1;
end
lum = lum / divide
m
n
divide
Xrgb